{
  "name": "sell_eletronic",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "npm run typings",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings install",
    "clean": "rm -rf dist",
    "serve": "http-server . -p 5556",
    "build": "npm run clean && tsc",
    "build_prod": "npm run build && browserify -s main dist/main.js > dist/bundle.js && npm run minify"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "angular2-in-memory-web-api": "0.0.20",
    "angular2-resizable": "^0.3.3",
    "bootstrap": "^3.3.6",
    "browserify": "^13.1.0",
    "core-js": "^2.4.1",
    "http-server": "^0.9.0",
    "mongodb": "^2.2.10",
    "mongoose": "^4.6.1",
    "ng2-responsive": "^0.4.6",
    "node-debug": "^0.1.0",
    "node-inspector": "^0.12.8",
    "node-schedule": "^1.1.1",
    "reflect-metadata": "^0.1.3",
    "reminder": "^0.1.1",
    "request": "^2.75.0",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "uglifyjs": "^2.4.10",
    "webserver": "^1.1.2",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-html-replace": "^1.6.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^3.0.1",
    "gulp-uglify": "^2.0.0",
    "lite-server": "^2.2.2",
    "nightmare": "^2.7.0",
    "systemjs-builder": "^0.15.32",
    "typescript": "^2.0.2",
    "typings": "^1.3.2"
  }
}
